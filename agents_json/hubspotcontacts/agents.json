                "properties": {
                  "properties": {
                    "type": "object",
                    "description": "Contact properties to update.",
                    "properties": {
                      "company": { "type": "string" },
                      "email": { "type": "string" },
                      "firstname": { "type": "string" },
                      "lastname": { "type": "string" },
                      "phone": { "type": "string" },
                      "website": { "type": "string" }
                    }
                  }
                },
                "required": ["properties"]
              }
            }
          },
          "required": true
        },
        "responses": {
          "success": {
            "type": "object",
            "description": "Updated contact details after the get and update flow.",
            "example": {
              "id": "contactId123",
              "properties": {
                "email": "updated@example.com"
              },
              "updatedAt": "2022-03-03T00:00:00Z"
            }
          }
        }
      }
    }
  ]
